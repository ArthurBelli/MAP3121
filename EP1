import matplotlib.pyplot as plt
import numpy as np

A = np.matrix([[1,2,3],[4,5,6]])

### decomposição LU

def decompLU(A):  

    U = np.copy(A)  
    n = np.shape(U)[0]  
    L = np.eye(n)  

    for j in np.arange(n-1):  
        for i in np.arange(j+1,n):  
            L[i,j] = U[i,j]/U[j,j]  
            for k in np.arange(j+1,n):  
                U[i,k] = U[i,k] - L[i,j]*U[j,k]  
            U[i,j] = 0  
    
    L.flatten()
    U.flatten()
    A.flatten()

    return L, U
