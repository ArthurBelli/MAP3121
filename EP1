import matplotlib.pyplot as plt
import numpy as np


### decomposição LU

A = np.matrix([[1,2,3],[4,5,6]]) ## deve ser input no projeto final

U = np.copy(A)
n = np.shape(U)[0]
L = np.eye(n)
 
for i in range (1,n):   
    U[i,i:n] = A[i,i:n] - L[i,1:(i-1)] * U[1:(i-1),i:n]
    L[(i+1):n,i] = 1/U[i,i] (A[(i+1):n,i] - L[(i+1):n,1:(i-1)] * U[1:(i-1),i])

##OBS: ta dando um erro relacionado ao tamanho da matriz
